{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "outDir": "./dist",
    "declaration": false,
    "declarationMap": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitOverride": false,
    "isolatedModules": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
